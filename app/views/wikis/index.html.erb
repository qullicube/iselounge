
  <div id="wiki" class="body" class="display:none">
  <div id="main" class="row-fluid">
    <div id="curriculum" class="span4">
        <ul class="big-show">
          <li class="active">
              <div class="content" data-catid="0">
                <h4>ICE</h4>
                Information and Communication
              </div>
              <div class="fui-arrow-right"></div>  
          </li>
          <li>
              <div class="content" data-catid="1">
                <h4>AERO</h4>
                Aerospace
              </div>
              <div class="fui-arrow-right"></div>  
          </li>
          <li>
              <div class="content" data-catid="2">
                <h4>ADME</h4>
                Automotive Design and Manufacturing
              </div>
              <div class="fui-arrow-right"></div>  
          </li>
          <li>
              <div class="content" data-catid="3">
                <h4>NANO</h4>
                Nano Technology
              </div>
              <div class="fui-arrow-right"></div>  
          </li>
            <li>
              <div class="content" data-catid="4">
                <h4>Others</h4>
                General Education
              </div>
              <div class="fui-arrow-right"></div>  
          </li>
        </ul>
    </div>
    <div id="year" class="span4">
        <ul class="small-show">
        <% 4.times do |y| %>
          <% 2.times do |s| %>
          <li><%= render partial: "col2", :locals => {:year => y+1, :semester => s+1} %></li>
          <% end %>
        <% end %>
        </ul>
    </div>
    <div id="course" >
      <ul class="small-show">
      </ul>
    </div>
  </div>
  <div class="row-fluid" style="height:0px">
    <div class="span12" style="height:0px">
    <div id="search-tool" class="span4">
          <div class="footer search">
            <!-- AJAX CALL ONLY -->
              <div class="span10"><input id="search-course" type="search" placeholder="Find your course" class="pull-left"></div>
              <div class="fui-search pull-right"></div>
          </div>
    </div>
    </div>
  </div>
  </div>
  </div>
  <div id="course-viewer" style="background:blue;"></div>
